<div class="frank">
    <div class="container-header">
        <div class="frank-logo">
            <a href="#">Frank</a>
        </div>
        <div class="frank-shipping">
            <a href="{$shipping}" class="shipping-link">Shipping</a>
        </div>
        <div class="frank-returns">
            <a href="{$returns}" class="returns-link">Returns</a>
        </div>
        <div class="frank-chart">
            <a href="#"><i class="material-icons" id="settings">settings</i></a>
        </div>
        <div class="frank-settings">
            <a href="#"><i class="material-icons">assessment</i></a>
        </div>
    </div>

    <div class="settings-boxes">
        <form action="">
            <div class="contact-details-box">
                <div class="contact-detail-heading">
                    <p>Contact detail</p>
                </div>
                <div class="contact-person-label">
                    <label for="">Contact person</label>
                </div>
                <div class="phone-label">
                    <label for="">Phone</label>
                </div>
                <div class="language-label">
                    <label for="">Language</label>
                </div>
                <div class="contact-person-text">
                    <input type="text">
                </div>
                <div class="phone-text">
                    <input type="text">
                </div>
                <div class="language-text">
                    <input type="text">
                </div>
                <div class="all-text-save">
                    <button>Save</button>
                </div>
            </div>
        </form>
        <div class="menu-box">
            <div class="menu-account">
                <a href="#">Account</a>
            </div>
            <div class="menu-invoice">
                <a href="#">Invoice</a>
            </div>
            <div class="menu-warehouse">
                <a href="#">Warehouse</a>
            </div>
        </div>
        <div class="email-address-box">
            <div class="email-address-heading">
                <p>Email address</p>
            </div>
            <div class="email-table">
                <p>Email</p>
            </div>
            <div class="role-table">
                <p>Role</p>
            </div>
            <div class="company-table">
                <p>Company</p>
            </div>
            <div class="status-table">
                <p>Status</p>
            </div>
            <div class="add-new-email-address-label">
                <label for="">Add new email address</label>
            </div>
            <div class="add-new-role-label">
                <label for="">Add new role</label>
            </div>
            <div class="add-new-email-address-text">
                <input type="text">
            </div>
            <div class="add-new-role-text">
                <input type="text">
            </div>
            <div class="add-button">
                <button>Add</button>
            </div>
        </div>
        <div class="change-password-box">
            <div class="change-password-heading">
                <p>Change password</p>
            </div>

            <div class="current-password-label">
                <label for="">Current password</label>
            </div>
            <div class="new-password-label">
                <label for="">New password</label>
            </div>
            <div class="confirm-password-label">
                <label for="">Confirm password</label>
            </div>

            <div class="current-password-text">
                <input type="text">
            </div>
            <div class="new-password-text">
                <input type="text">
            </div>
            <div class="confirm-password-text">
                <input type="text">
            </div>

            <div class="save-button">
                <button>Save</button>
            </div>
        </div>
        <div class="delete-account-box">
            <div class="delete-account-heading">
                <p>Delete account</p>
            </div>
            <div class="delete-info">
                <p>Once you request your account to be deleted you have until November 29. 2020 to keep this account</p>
            </div>
            <div class="delete-note">
                <p>Please note:</p>
                <ul>
                    <li>You will not be able to create shipping anymore</li>
                    <li>All you account data will be permanently removed in 6 months</li>
                    <li>We recommend you to export you data before deleting this account</li>
                </ul>
            </div>
            <div class="delete-account-button">
                <button>Delete account</button>
            </div>
        </div>
    </div>
</div>
